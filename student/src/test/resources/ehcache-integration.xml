<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
	maxBytesLocalHeap="200m" eternal="false" memoryStoreEvictionPolicy="LFU"
	transactionalMode="off">
    <cache name="shortTerm" timeToLiveSeconds="2" />
    <cache name="mediumTerm" timeToLiveSeconds="5" />
    <cache name="longTerm" timeToLiveSeconds="10" />
</ehcache>