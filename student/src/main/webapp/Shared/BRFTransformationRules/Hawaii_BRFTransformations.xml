<?xml version="1.0" encoding="utf-8"?>
<TransformationRules>
  <Tag>
    <ContextType>Illustration</ContextType>
    <TagName>p</TagName>
    <Attributes>
      <Content>&#xA0;</Content>
      <Attribute>
        <Name>style</Name>
        <Value>*</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <Keepchildren>none</Keepchildren>
      </Transform>
    </Transformations>
  </Tag>
  <!-- start italics-->
  <Tag>
    <ContextType>All</ContextType>
    <TagName>span</TagName>
    <Attributes>
      <Attribute>
        <Name>style</Name>
        <Value>font-style:italic;</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <NewTag>
          <TagName>i</TagName>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <Tag>
    <ContextType>All</ContextType>
    <TagName>span</TagName>
    <Attributes>
      <Attribute>
        <Name>style</Name>
        <Value>FONT-WEIGHT: normal; FONT-STYLE: italic</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <NewTag>
          <TagName>i</TagName>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <Tag>
    <ContextType>All</ContextType>
    <TagName>p</TagName>
    <Attributes>
      <Attribute>
        <Name>style</Name>
        <Value>font-style:italic;</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <NewTag>
          <TagName>i</TagName>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <Tag>
    <ContextType>All</ContextType>
    <TagName>p</TagName>
    <Attributes>
      <Attribute>
        <Name>style</Name>
        <Value>FONT-WEIGHT: normal; FONT-STYLE: italic</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <NewTag>
          <TagName>i</TagName>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <Tag>
    <ContextType>All</ContextType>
    <TagName>span</TagName>
    <Attributes>
      <Attribute>
        <Name>style</Name>
        <Value>text-decoration:underline;</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <NewTag>
          <TagName>i</TagName>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <Tag>
    <ContextType>All</ContextType>
    <TagName>span</TagName>
    <Attributes>
      <Attribute>
        <Name>style</Name>
        <Value>font-weight:bold;</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <NewTag>
          <TagName>i</TagName>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <!-- end italics-->
  <Tag>
    <ContextType>All</ContextType>
    <TagName>p</TagName>
    <Attributes>
      <Attribute>
        <Name>style</Name>
        <Value></Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <Occurrence>1</Occurrence>
        <NewTag>
          <TagName>p</TagName>
          <Attributes>
            <Attribute>
              <Name>style</Name>
              <Value>firstpara</Value>
            </Attribute>
          </Attributes>
        </NewTag>
      </Transform>
      <Transform>
        <NewTag>
          <TagName>p</TagName>
          <Attributes>
            <Attribute>
              <Name>style</Name>
              <Value>secondpara</Value>
            </Attribute>
          </Attributes>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <Tag>
    <ContextType>All</ContextType>
    <TagName>p</TagName>
    <Attributes>
      <Attribute>
        <Name>style</Name>
        <!-- this is actually used as a passage title and that is why i have a separate rule in case we 
    want to handle this differently in the future.-->
        <Value>text-decoration:underline;</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <Occurrence>1</Occurrence>
        <NewTag>
          <TagName>p</TagName>
          <Attributes>
            <Attribute>
              <Name>style</Name>
              <Value>firstpara</Value>
            </Attribute>
          </Attributes>
        </NewTag>
      </Transform>
      <Transform>
        <NewTag>
          <TagName>p</TagName>
          <Attributes>
            <Attribute>
              <Name>style</Name>
              <Value>secondpara</Value>
            </Attribute>
          </Attributes>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <Tag>
    <ContextType>All</ContextType>
    <TagName>div</TagName>
    <Attributes>
      <Attribute>
        <Name>class</Name>
        <Value>*</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <NewTag>
          <TagName>p</TagName>
          <Attributes>
            <Attribute>
              <Name>style</Name>
              <Value>secondpara</Value>
            </Attribute>
          </Attributes>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <Tag>
    <ContextType>Stem</ContextType>
    <TagName>div</TagName>
    <Attributes>
      <Attribute>
        <Name>class</Name>
        <Value>attribute</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <Keepchildren>all</Keepchildren>
        <NewTag>
          <TagName>div</TagName>
          <Attributes>
            <Attribute>
              <Name>class</Name>
              <Value>attribute</Value>
            </Attribute>
          </Attributes>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <Tag>
    <ContextType>Option</ContextType>
    <TagName>p</TagName>
    <Attributes>
      <Attribute>
        <Name>style</Name>
        <Value>*</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <NewTag>
          <TagName>span</TagName>
          <Attributes>
            <Attribute>
              <Name>style</Name>
              <Value>firstpara</Value>
            </Attribute>
          </Attributes>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <!--the next two are for author: to be centered. -->
  <Tag>
    <ContextType>Passage</ContextType>
    <TagName>p</TagName>
    <Attributes>
      <Attribute>
        <Name>style</Name>
        <Value>text-align:center; font-weight:bold;</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <NewTag>
          <TagName>p</TagName>
          <Attributes>
            <Attribute>
              <Name>style</Name>
              <Value>arith</Value>
            </Attribute>
          </Attributes>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <Tag>
    <ContextType>Passage</ContextType>
    <TagName>p</TagName>
    <Attributes>
      <Attribute>
        <Name>style</Name>
        <Value>background-color:transparent; text-align:center; font-weight:bold;</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <NewTag>
          <TagName>p</TagName>
          <Attributes>
            <Attribute>
              <Name>style</Name>
              <Value>arith</Value>
            </Attribute>
          </Attributes>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <!-- subheading: should be formatted in cell 5. boldface should not be shown-->
  <Tag>
    <ContextType>Passage</ContextType>
    <TagName>p</TagName>
    <Attributes>
      <Attribute>
        <Name>style</Name>
        <Value>font-weight:bold;</Value>
      </Attribute>
    </Attributes>
    <Transformations>
      <Transform>
        <NewTag>
          <TagName>p</TagName>
          <Attributes>
            <Attribute>
              <Name>style</Name>
              <Value>subheading</Value>
            </Attribute>
          </Attributes>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <!--next two rules deal with numbered paragraphs numbered paragraphs.-->
  <Tag>
    <ContextType>Passage</ContextType>
    <TagName>dt</TagName>
    <Attributes></Attributes>
    <Transformations>
      <Transform>
        <NewTag>
          <TagName>dt</TagName>
          <Attributes>
            <Attribute>
              <Name>class</Name>
              <Value>numberedpara</Value>
            </Attribute>
          </Attributes>
        </NewTag>
      </Transform>
    </Transformations>
  </Tag>
  <!-- there are empty paragraph tags all over the place. -->
  <Tag>
    <ContextType>Passage</ContextType>
    <TagName>p</TagName>
    <Attributes>
      <Content>&#xA0;</Content>
    </Attributes>
    <Transformations>
      <Transform>
        <Keepchildren>none</Keepchildren>
      </Transform>
    </Transformations>
  </Tag>
</TransformationRules>

