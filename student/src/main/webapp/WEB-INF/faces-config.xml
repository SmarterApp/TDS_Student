		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>reviewShellBacking</managed-bean-name>
		<managed-bean-class>tds.student.web.backing.ReviewShellBacking</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>notificationBacking</managed-bean-name>
		<managed-bean-class>tds.student.web.backing.NotificationBacking</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<!-- Todo: Shiva Removing this component throws an error e.g. PlaceHolderTDS component cannot be found. look here: http://stackoverflow.com/questions/9684402/jsf-custom-component-is-not-found -->
	<component>
		<component-type>PlaceHolderTDS</component-type>
		<component-class>AIR.Common.Web.taglib.PlaceHolder</component-class>
	</component>
	<component>
		<component-type>PlaceHolder</component-type>
		<component-class>AIR.Common.Web.taglib.PlaceHolder</component-class>
